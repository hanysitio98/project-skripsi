  "scripts": {
    "start": "env-cmd -f ./.env.development react-scripts start",
    "build": "env-cmd -f ./.env.production react-scripts build",
    "deploy:sit": "env-cmd -f ./.env.sit react-scripts build && XCOPY Staticfile build && cf push -f manifest-dev.yml",
